generator cacheClient {
  provider = "prisma-client-js"
  output   = "./gen/cache"
}

datasource cache {
  provider = "sqlite"
  url      = "file:./cache.db"
}

model CacheEntry {
  id        String  @id @default(uuid())
  key       String  @unique
  value     String
  expiresAt BigInt?  // <-- change here
  createdAt BigInt   // <-- change here
}
