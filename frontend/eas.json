{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "GOOGLE_SERVICES_FILE": "$GOOGLE_SERVICES_JSON"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "GOOGLE_SERVICES_FILE": "$GOOGLE_SERVICES_JSON"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_FILE": "$GOOGLE_SERVICES_JSON"
        }
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
