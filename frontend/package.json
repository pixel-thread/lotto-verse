{
  "name": "lotto verse",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "local:dev:android": "eas build --profile development --platform android --local",
    "local:preview:android": "eas build --profile preview --platform android --local",
    "local:prod:android": "eas build --profile production --platform android --local",
    "build:android:dev": "eas build --profile development --platform android",
    "build:android:preview": "eas build --profile preview --platform android",
    "build:android:prod": "eas build --profile production --platform android",
    "update:preview": "eas update --channel preview --message \"Preview update\"",
    "update:prod": "eas update --channel production --message \"Production update\"",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.18.3",
    "@expo/vector-icons": "^15.0.2",
    "@hookform/resolvers": "^5.2.2",
    "@react-navigation/drawer": "^7.5.0",
    "@react-navigation/native": "^7.1.6",
    "@tamagui/babel-plugin": "1.136.9",
    "@tamagui/config": "1.136.9",
    "@tanstack/react-query": "^5.90.7",
    "axios": "^1.13.2",
    "expo": "54.0.23",
    "expo-auth-session": "~7.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.17",
    "expo-font": "~14.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.7",
    "nativewind": "latest",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-razorpay": "^2.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "sonner-native": "^0.21.1",
    "tamagui": "^1.136.9",
    "zod": "^3.25.76",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "@types/react-native-razorpay": "^2.2.6",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2"
  },
  "private": true
